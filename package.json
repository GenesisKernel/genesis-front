    "scripts": {
        "extract-locales": "node util/extractLocales.js",
        "build-css": "node-sass-chokidar --include-path ./src/app/styles/scss --include-path ./node_modules src/app/styles/scss/sass.scss -o src/app/styles/built",
        "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/app/styles/scss --include-path ./node_modules src/app/styles/scss/sass.scss -o src/app/styles/built --watch --recursive",
        "start-js": "react-scripts-ts start",
        "start": "cross-env REACT_APP_VERSION=$npm_package_version npm-run-all -p watch-css start-js",
        "build": "npm run build-css && cross-env REACT_APP_VERSION=$npm_package_version react-scripts-ts build",
        "test": "react-scripts-ts test --env=jsdom",
        "eject": "react-scripts-ts eject",
        "start-desktop": "cross-env REACT_APP_VERSION=$npm_package_version npm-run-all -p watch-css start-js-desktop",
        "start-js-desktop": "react-scripts-ts-electron start",
        "build-desktop": "npm run build-css && cross-env PUBLIC_URL=./ REACT_APP_VERSION=$npm_package_version react-scripts-ts-electron build",
        "package": "electron-builder --config electron-builder.json --dir",
        "release": "electron-builder --config electron-builder.json"
    },
